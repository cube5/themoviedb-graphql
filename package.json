{
  "name": "themoviedb-graphql",
  "version": "1.0.0",
  "description": "Graphql wrapper for TheMovieDB API.",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec 'ncc run' src/server.ts",
    "build": "ncc build src/server.ts --minify --source-map",
    "gql:codegen": "graphql codegen",
    "gql:get-schema": "graphql get-schema --project themoviedb -e dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-datasource-rest": "^0.3.2",
    "apollo-server": "^2.4.8",
    "graphql": "^14.2.1",
    "graphql-tools": "^4.0.4"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.18.1",
    "graphql-binding": "^2.5.1",
    "graphql-cli": "^3.0.11",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
